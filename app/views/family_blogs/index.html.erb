<h1>Listing family_blogs</h1>

<table>
  <tr>
    <th>Family</th>
    <th>Blog</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @family_blogs.each do |family_blog| %>
  <tr>
    <td><%= family_blog.family_id %></td>
    <td><%= family_blog.blog %></td>
    <td><%= link_to 'Show', family_blog %></td>
    <td><%= link_to 'Edit', edit_family_blog_path(family_blog) %></td>
    <td><%= link_to 'Destroy', family_blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

